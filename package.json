  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel start:dev lint:watch test:watch",
    "start:dev": "babel-node internals/scripts/devServer.js",
    "lint": "npm run lint:js",
    "lint:eslint": "node_modules/.bin/esw --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV='test' mocha --reporter spec internals/scripts/testsSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm test -- --watch",
    "clean:dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "prebuild": "npm-run-all clean:dist test lint",
    "build": "cross-env NODE_ENV='production' babel-node buildScripts/build.js"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged",
