{
  "name": "is-bp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel start:dev lint:css lint:watch",
    "start:dev": "cross-env NODE_ENV='development' babel-node internals/scripts/devServer.js",
    "lint": "npm-run-all --parallel lint:js lint:css",
    "lint:stylelint": "stylelint **/*.scss --syntax scss --color",
    "lint:eslint": "esw --ignore-path .gitignore --ignore-pattern internals/scripts --color",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:css": "npm run lint:stylelint",
    "lint:staged": "lint-staged",
    "lint:watch": "npm run lint:js -- --watch",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:fix-css": "stylefmt --config .stylelintrc.json  src/**/*.scss",
    "test": "cross-env NODE_ENV='test' jest --config internals/testing/jest.config.json",
    "test:watch": "npm test -- --watch",
    "clean:dist": "npm run remove-dist && mkdir dist",
    "prestart-mockapi": "babel-node internals/mock/generateMockData",
    "start-mockapi": "babel-node internals/mock/jsonServer --watch",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "manage:translations": "babel-node internals/scripts/manageTranslations",
    "prebuild": "npm-run-all clean:dist test lint",
    "build": "cross-env NODE_ENV='production' babel-node internals/scripts/build.js",
    "sandbox": "babel-node internals/scripts/sandbox"
  },
  "lint-staged": {
    "*.js": "lint:eslint",
    "*.scss": "lint:stylelint"
  },
  "pre-commit": "lint:staged",
  "browserslist": [
    "> 1%",
    "last 2 version",
    "not OperaMini  all",
    "not ie < 11",
    "not ExplorerMobile < 11",
    "not UCAndroid <= 100"
  ],
